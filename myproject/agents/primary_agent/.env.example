# Primary Agent - Environment Variables Example
# Copy this file to .env and fill in your specific values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o

# General Settings
ENABLE_DETAILED_LOGGING=true
DEBUG_MODE=false

# Instagram API Credentials
INSTAGRAM_API_KEY=your_instagram_api_key
INSTAGRAM_API_SECRET=your_instagram_api_secret
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
INSTAGRAM_BUSINESS_ID=your_instagram_business_id

# Twitter/X API Credentials
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret

# LinkedIn API Credentials
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token

# Facebook API Credentials
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token

# Slack API Credentials
SLACK_BOT_TOKEN=your_slack_bot_token
SLACK_APP_TOKEN=your_slack_app_token
SLACK_SIGNING_SECRET=your_slack_signing_secret

# Brave Search API (for web search)
BRAVE_SEARCH_API_KEY=your_brave_search_api_key

# Airtable API (for database)
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_airtable_base_id

# Gmail API (for email communications)
GMAIL_CREDENTIALS_FILE=path_to_credentials.json
GMAIL_TOKEN_FILE=path_to_token.json

# Google Drive API (for file storage)
GOOGLE_DRIVE_CREDENTIALS_FILE=path_to_credentials.json
GOOGLE_DRIVE_TOKEN_FILE=path_to_token.json

# MCP Server Configuration
MCP_HOST=localhost
MCP_PORT=8080
MCP_SECRET_KEY=your_mcp_secret_key

# Agent Settings
PRIMARY_AGENT_TIMEOUT_SECONDS=60
MAX_CONCURRENT_SUBAGENTS=3

# File Paths
LOGS_PATH=logs
DATA_PATH=data

# Default System Prompt
# Uncomment and customize if you want to override the default system prompt
# SYSTEM_PROMPT=Your custom system prompt here 